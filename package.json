{
  "name": "sadad-uikit",
  "version": "1.0.1",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "uikit",
    "storybook",
    "components"
  ],
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "tailwindcss": ">=3 <4"
  },
  "devDependencies": {
    "@storybook/addon-docs": "8.6.14",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/cli": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.13",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "6.3.5"
  },
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build -o ./.storybook-static",
    "build:lib": "tsup",
    "build:css": "npx tailwindcss -c tailwind.config.cjs -i ./src/styles/styles.css -o ./dist/styles.css --minify",
    "build": "npm run build:lib && npm run build:css",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "sideEffects": ["**/*.css"]
}
